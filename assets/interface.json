{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "Default",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/../agent/main.py"
        ]
    },
    "task": [
        {
            "name": "森空岛签到",
            "entry": "entry_signSkland",
            "option": [
                "关闭森空岛"
            ]
        },
        {
            "name": "小黑盒签到",
            "entry": "entry_signXiaoheihe"
        },
        {
            "name":"游侠网",
            "entry": "entry_signYouxia"   
        },
        {
            "name":"微博超话",
            "entry": "entry_signWeiboST"
        }
    ],
    "option": {
        "关闭森空岛": {
            "cases": [
                {
                    "name": "启用",
                    "pipeline_override": {
                        "CloseSkland": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "禁用",
                    "pipeline_override": {
                        "CloseSkland": {
                            "enabled": false
                        }
                    }
                }
            ]
        }
    }
}