{
    //Todo
    //游侠App签到确认很慢，必须有完签确认“今天+已签到”
    // 检测是否获取CDKEY等奖励
    "entry_signYouxia": {
        "next": [
            "Youxia-flagSignPage",
            "Youxia-enterSignPage1",
            "Youxia-enterSignPage2",
            "Youxia-enterMyPage"
        ],
        "interrupt": "startYouxia"
    },
    "Youxia-enterMyPage": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "Youxia/enterMyPage.png",
                "roi": [
                    560,
                    1421,
                    160,
                    180
                ]
            }
        },
        "action": "Click",
        "next": [
            "Youxia-enterSignPage1",
            "Youxia-enterSignPage2",
            "Youxia-enterMyPage"
        ]
    },
    "Youxia-enterSignPage1": {
        "recognition": {
            "type": "OCR",
            "param": {
                //"template": "Youxia/enterSignYouxia.png",
                "expected": "前往领取",
                "roi": [
                    395,
                    518,
                    325,
                    258
                ]
            }
        },
        "action": "Click",
        "next": ["Youxia-flagSignPage","Youxia-enterSignPage2"]
    },
    "Youxia-enterSignPage2": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "Youxia/entranceSign2.png",
                "roi": [
                    228,
                    935,
                    263,
                    172
                ]
            }
        },
        "action": "Click",
        "next": "Youxia-flagSignPage"
    },
    "Youxia-flagSignPage": {
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": "游侠福利月历",
                "roi": [
                    209,
                    24,
                    302,
                    150
                ]
            }
        },
        "next": [
            "Youxia-sign",
            "Youxia-findLotteryButton"
        ],
        "interrupt": "Youxia-swipeUp",
        "post_delay": 1500,
        "on_error": "FullScreen-swipeUp",
        "timeout": 10000
    },
    "Youxia-sign": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "Youxia/buttonSign.png",
                "roi": [
                    30,
                    226,
                    658,
                    1300
                ],
                "threshold": 0.9
            }
        },
        "action": "Click",
        "next": ["Youxia-result_SignSucceed"],
        "on_error": ["Youxia-flagSignPage"],
        "timeout": 3000
    },
    "Youxia-result_SignSucceed":{
        "recognition": {
            "type": "OCR",
            "param":{
                "expected": "签到成功",
                "roi":[322,1359,142,75]                
            }
        },
        "action": "Custom",
        "custom_action": "delay_focus_hook",
        "custom_action_param": "key=任务成功&focus=游侠网签到成功",
        "next": ["Youxia-flagSignPage"]
    },
    "Youxia-swipeUp": {
        "action": {
            "type": "Swipe",
            "param": {
                "begin": [
                    362,
                    1528,
                    1,
                    1
                ],
                "end": [
                    362,
                    153,
                    1,
                    1
                ],
                "duration": 500
            }
        },
        "post_wait_freezes": 300
    },
    "Youxia-findLotteryButton": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "Youxia/buttonLottery.png",
                "roi": [
                    226,
                    1062,
                    269,
                    184
                ]
            }
        },
        "next": "Youxia-recoButtonColor",
        "timeout": 5000,
        "on_error": "Youxia-result_NoLottery"
    },
    "Youxia-result_NoLottery":{
        "action": "Custom",
        "custom_action": "delay_focus_hook",
        "custom_action_param": "key=任务成功&focus=游侠网无抽奖",
        "next": "stopYouxia"
    },
    "Youxia-recoButtonColor":{
        "recognition": {
            "type": "ColorMatch",
            "param":{
                "roi":"Youxia-findLotteryButton",
                "method": 4,
                "lower": [210,60,0],
                "upper": [255,200,60]
            }
        },
        "action": "Click",
        "post_wait_freezes": 1000,
        "next": "Youxia-ReceiveLottery",
        "timeout": 10000,
        "on_error": "FullScreen-swipeUp"
    },
    "Youxia-ReceiveLottery": {
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": "领取",
                "roi": [
                    257,
                    1043,
                    214,
                    150
                ]
            }
        },
        "action": "Click",
        //"next": "Youxia-flagSignPage",
        "next": "Youxia-recoRedeemCode",
        "post_delay": 1500
        //"interrupt": "FullScreen-swipeBack"
    },
    "Youxia-recoRedeemCode":{
        "action": "Custom",
        "custom_action": "delay_focus_hook",
        "custom_action_param": "key=任务成功&focus=游侠网有抽奖",
        "next": "FullScreen-swipeUp"
    },
    "startYouxia": {
        "action": "StartApp",
        "package": "net.ali213.YX"
    },
    "stopYouxia": {
        "action": "StopApp",
        "package": "net.ali213.YX"
    }
}