{ //等28号验证周签奖励领取
    "entry_signWsgw": {
        "next": [
            "Wsgw-closePopWindow",
            "Wsgw-findSignButton1",
            "Wsgw-enterMyPage"
        ],
        "interrupt": [
            "startWsgw"
        ]
    },
    "Wsgw-closePopWindow": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "Wsgw/buttonClosePopWindow.png",
                "roi": [
                    545,
                    284,
                    135,
                    135
                ]
            }
        },
        "action": "Click",
        "next": "entry_signWsgw"
    },
    "Wsgw-findSignButton1": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "Wsgw/signEntrance1.png",
                "roi": [
                    261,
                    815,
                    196,
                    223
                ]
            }
        },
        "action": "Click",
        "next": "Wsgw-checkSignPage"
    },
    "Wsgw-enterMyPage": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "Wsgw/buttonMyPage.png",
                "roi": [
                    568,
                    1432,
                    152,
                    169
                ]
            }
        },
        "action": "Click",
        "next": "Wsgw-findSignButton2"
    },
    "Wsgw-findSignButton2": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "Wsgw/signEntrance2.png",
                "roi": [
                    513,
                    148,
                    207,
                    145
                ]
            }
        },
        "action": "Click",
        "next": "Wsgw-checkSignPage"
    },
    "Wsgw-checkSignPage": {
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": "每日签到",
                "roi": [
                    226,
                    72,
                    272,
                    72
                ]
            }
        },
        "next": "Wsgw-checkWeekAwards",
        "interrupt": "Wsgw-result_SignSucceed",
        "timeout": 5000,
        "on_error": "FullScreen-swipeUp"
    },
    "Wsgw-result_SignSucceed":{
        "recognition": {
            "type": "OCR",
            "param":{
                "expected": "签到成功",
                "roi":[254,566,204,65]
            }
        },
        "action": "Custom",
        "custom_action": "delay_focus_hook",
        "custom_action_param": "key=任务成功&focus=网上国网签到成功",
        "next": "Wsgw-clickReceive"
    },
    "Wsgw-clickReceive": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "Wsgw/buttonReceive.png",
                "roi": [
                    218,
                    868,
                    273,
                    159
                ]
            }
        },
        "action": "Click"
    },
    "Wsgw-checkWeekAwards": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "Wsgw/buttonWeekAward1.png",
                "roi": [
                    37,
                    988,
                    643,
                    79
                ],
                "threshold": 0.8
            }
        },
        "action": "Click",
        "target_offset": [0,70,0,0],
        "post_delay": 3000,
        "next": ["Wsgw-result_NoWeekAward"]
        //"next": "FullScreen-swipeUp"
    },
    "Wsgw-result_NoWeekAward":{
        "recognition": {
            "type": "OCR",
            "param":{
                "expected": ["很遗憾"],
                "roi":[132,756,116,53]                
            }
        },
        "action": "Custom",
        "custom_action": "delay_focus_hook",
        "custom_action_param": "key=任务成功&focus=网上国网无周礼包",
        "next":"stopWsgw"
    },
    /*
    "Wsgw-result_GetWeekAward":{
        "recognition": {
            "type": "OCR",
            "param":{
                "expected": ["很遗憾"],
                "roi":[132,756,116,53]                
            }
        },
        "action": "Custom",
        "custom_action": "delay_focus_hook",
        "custom_action_param": "key=任务成功&focus=网上国网无周礼包",
        "next":"stopWsgw"
    },
    */
    "startWsgw": {
        "action": "StartApp",
        "package": "com.sgcc.wsgw.cn"
    },
    "stopWsgw": {
        "action": "StopApp",
        "package": "com.sgcc.wsgw.cn"
    }
}