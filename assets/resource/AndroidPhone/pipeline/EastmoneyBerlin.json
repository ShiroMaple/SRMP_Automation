{ //目前无新债可申购，暂时上滑；出现可打新的情况时再继续编辑
    "entry_signEastmoneyBerlin": {
        "next": [
            //"EastmoneyBerlin-result_打新成功",
            "EastmoneyBerlin-enterTradePage"
        ],
        "interrupt": [
            //"skipAD_LowerRightCorner",    
            "startEastmoneyBerlin"
        ]
    },
    "EastmoneyBerlin-enterTradePage": {
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": "交易",
                "roi": [
                    579,
                    1475,
                    141,
                    126
                ]
            }
        },
        "action": "Click",
        "next": [
            "EastmoneyBerlin-flagTradePage",
            "EastmoneyBerlin-enterTradePage"
        ]
    },
    "EastmoneyBerlin-flagTradePage": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "Eastmoney/flagTradePage.png",
                "roi": [
                    571,
                    1429,
                    149,
                    172
                ]
            }
        },
        "next": "EastmoneyBerlin-enterIPOPage",
        "post_delay": 1500
    },
    "EastmoneyBerlin-enterIPOPage": {
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": "打新专区",
                "roi": [
                    108,
                    472,
                    223,
                    142
                ]
            }
        },
        "action": "Click",
        "next": [
            "EastmoneyBerlin-enterNewBondsPage",
            "EastmoneyBerlin-enterIPOPage"
        ],
        "post_delay": 2000
    },
    "EastmoneyBerlin-enterNewBondsPage": {
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": "新债",
                "roi": [
                    79,
                    354,
                    201,
                    172
                ]
            }
        },
        "action": "Click",
        "next": "EastmoneyBerlin-flagNewBondsPage"
    },
    "EastmoneyBerlin-flagNewBondsPage": {
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "template": "Eastmoney/flagNewBondsPageChosen.png",
                "roi": [
                    80,
                    352,
                    193,
                    171
                ]
            }
        },
        "post_delay": 1,
        "next": [
            "EastmoneyBerlin-新债预约申购Step1",
            "EastmoneyBerlin-result_无新债",
            "EastmoneyBerlin-enterNewBondsPage"
        ]
    },
    "EastmoneyBerlin-新债预约申购Step1":{
        "recognition": {
            "type": "OCR",
            "param":{
                "expected" : "键预约申购",
                "roi" : [409,443,311,202]
            }
        },
        /*
        "action": "Click",
        "next": "EastmoneyBerlin-新债预约申购Step2",
        */
        "next": "EastmoneyBerlin-result_有新债",
        "post_delay": 500
    },
    "EastmoneyBerlin-result_有新债":{
        "pre_delay": 0,
        "post_delay": 0,        
        "action":"Custom",
        "custom_action": "delay_focus_hook",
        "custom_action_param": "tag=正常&msg=东方财富有可申购新债",
        "next": "FullScreen-swipeUp"
    },
    "EastmoneyBerlin-新债预约申购Step2":{   
        "recognition": {
            "type": "OCR",
            "param":{
                "expected" : "预约申购",
                "roi" : [73,1221,552,179]       //后续观察1只以上申购时按钮位置会不会偏移
            }
        },
        "action": "Click",
        "next": ["EastmoneyBerlin-result_打新成功"],
        "post_delay": 500

    },
    "EastmoneyBerlin-result_打新成功":{
        "pre_delay": 0,
        "post_delay": 0,        
        "action": "Custom",
        "custom_action": "delay_focus_hook",
        "custom_action_param": "tag=正常&msg=东方财富打新成功",
        "next": "stopEastmoneyBerlin"
    },
    "EastmoneyBerlin-result_无新债": {
        "recognition": {
            "type": "OCR",
            "param": {
                "expected": ["暂无新债可申购","已全部预约"],
                "roi": [
                    4,
                    489,
                    707,
                    876
                ]
            }
        },        
        "action": "Custom",
        "custom_action": "delay_focus_hook",
        "custom_action_param": "tag=正常&msg=东方财富无可购新债",
        "next": "stopEastmoneyBerlin"
    },
    "startEastmoneyBerlin": {
        "action": "StartApp",
        "package": "com.eastmoney.android.berlin"
    },
    "stopEastmoneyBerlin": {
        "action": "StopApp",
        "package": "com.eastmoney.android.berlin"
    }
}